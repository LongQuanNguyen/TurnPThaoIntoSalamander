{
    "manifest_version": 2,
    "name": "Biến Phương Thảo thành kì nhông",
    "version": "1.0",
    "description": "Replacing PTheo name aliases into lizzard emoji",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
        "16": "./lizard.png",
        "32": "./lizard.png",
        "48": "./lizard.png",
        "128": "./lizard.png"
    }
}